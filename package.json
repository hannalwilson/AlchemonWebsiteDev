{
  "name": "alchemon-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "staging": "vue-cli-service serve --mode staging",
    "deploy": "aws --region us-west-2 --profile websiteAdmin s3 sync ./dist s3://alchemon.net --delete",
    "invalidate": "aws cloudfront create-invalidation --distribution-id EGLR6IFF7KTUI --paths /*",
    "launch": "npm run build && npm run deploy && npm run invalidate"
  },
  "dependencies": {
    "@json-rpc-tools/utils": "^1.7.6",
    "@randlabs/myalgo-connect": "^1.1.3",
    "@walletconnect/client": "^1.7.8",
    "@walletconnect/node": "^1.0.0",
    "@walletconnect/qrcode-modal": "^1.7.8",
    "algorand-walletconnect-qrcode-modal": "^1.7.4",
    "axios": "^0.26.1",
    "buffer": "^6.0.3",
    "core-js": "^3.8.3",
    "crypto-browserify": "^3.12.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "path-browserify": "^1.0.1",
    "vue": "^3.2.13",
    "vue-confetti-explosion": "^1.0.1",
    "vue-p5": "^0.8.4",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0",
    "web3.storage": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-standard": "^6.1.0",
    "algosdk": "^1.16.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^8.0.3",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0"
  }
}
